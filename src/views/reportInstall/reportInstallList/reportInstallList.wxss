.tab-h {
  height: 100px;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 100px;
  font-size: 32px;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  color: #9a886e;
  background: -webkit-linear-gradient(top, #474139, #34312e);
  /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(bottom, #474139, #34312e);
  /* Opera 11.1 - 12.0 */
